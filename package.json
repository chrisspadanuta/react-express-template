{
  "name": "react-express-template",
  "version": "0.0.1",
  "description": "Starting point for a react/express js project",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development babel-node ./server/index.js",
    "build": "NODE_ENV=production npx env-cmd .env webpack --config ./webpack/config-prod.babel.js",
